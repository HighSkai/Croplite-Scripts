function experienceskill(p: player):
	if {xpneedupdate::%{_p}'s uuid%} is true:
		open chest inventory with 1 row named "&6&lExperience Skill" to {_p}
		wait 1 tick
		close {_p}'s inventory
		wait 1 tick
		open chest inventory with 1 row named "&6&lExperience Skill" to {_p}
		loop integers between 0 and 9:
			set slot loop-value of {_p}'s current inventory to {xpskill::%{_p}'s uuid%::%loop-integer%}
			set {xpneedupdate::%{_p}'s uuid%} to false
	else:
		open chest inventory with 1 row named "&6&lExperience Skill" to {_p}
		loop integers between 0 and 9:
			set slot loop-value of {_p}'s current inventory to {xpskill::%{_p}'s uuid%::%loop-integer%}
			
