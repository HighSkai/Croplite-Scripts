on Skript stop:
    loop all players:
        if {expmultibooster-active::%loop-player's uuid%} is true:
            set {expmultibooster-active::%loop-player's uuid%} to false
            subtract 1 from {expmulti::%loop-player's uuid%}
            
            give loop-player {boosters::expmulti1}
        if {superexpmultibooster-active::%loop-player's uuid%} is true:
            set {superexpmultibooster-active::%loop-player's uuid%} to false
            subtract {superexpmultibooster-addition::%loop-player's uuid%} from {expmulti::%loop-player's uuid%}

            give loop-player {boosters::expmulti2}
        if {cropmultibooster-active::%loop-player's uuid%} is true:
            set {cropmultibooster-active::%loop-player's uuid%} to false
            subtract 1 from {cropmulti::%loop-player's uuid%}

            give loop-player {boosters::cropmulti1}
        if {supercropmultibooster-active::%loop-player's uuid%} is true:
            set {supercropmultibooster-active::%loop-player's uuid%} to false
            subtract {supercropmultibooster-addition::%loop-player's uuid%} from {cropmulti::%loop-player's uuid%}

            give loop-player {boosters::cropmulti2}
        if {damagebooster-active::%loop-player's uuid%} is true:
            set {damagebooster-active::%loop-player's uuid%} to false
            remove 1 from {damageboost::%loop-player's uuid%}

            give loop-player {boosters::damage1}
        if {superdamagebooster-active::%loop-player's uuid%} is true:
            set {superdamagebooster-active::%loop-player's uuid%} to false
            remove 2 from {damageboost::%loop-player's uuid%}

            give loop-player {boosters::damage2}
        if {speedbooster-active::%loop-player's uuid%} is true:
            set {speedbooster-active::%loop-player's uuid%} to false
            subtract {speedbooster-addition::%loop-player's uuid%} from loop-player's walk speed

            give loop-player {boosters::speed1}
        if {superspeedbooster-active::%loop-player's uuid%} is true:
            set {superspeedbooster-active::%loop-player's uuid%} to false
            subtract {superspeedbooster-addition::%loop-player's uuid%} from loop-player's walk speed

            give loop-player {boosters::speed2}
            
local function setBoosters():
