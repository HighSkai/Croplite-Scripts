on script load:
    setBoosters()

on consume of potion:
    if name of event-item is "&e&lExp Multi-Up":
        if {expmultibooster-active::%player's uuid%} and {superexpmultibooster-active::%player's uuid%} are false:
            set {expmultibooster-active::%player's uuid%} to true
            set player's held item to air
            add 1 to {expmulti::%player's uuid%}
            send "&d&lCROPLITE &aYou have activated an &e&lExp Multi-Up &abooster." to player
            play sound "minecraft:block.brewing_stand.brew" with pitch 1.3 to player
            play sound "entity.experience_orb.pickup" to player
                        
            wait 8 minutes
            set {expmultibooster-active::%player's uuid%} to false
            subtract 1 from {expmulti::%player's uuid%}
            send "&d&lCROPLITE &cYour &e&lExp Multi-Up &cbooster has ran out." to player
            play sound "minecraft:entity.villager.no" to player
        else:
            if {expmultibooster-active::%player's uuid%} is true:
                send "&d&lCROPLITE &cYou already have an active &e&lExp Multi-Up &cbooster." to player
                play sound "minecraft:entity.villager.no" to player
            else:
                send "&d&lCROPLITE &cYou already have an active &e&lSuper Exp Multi-Up &cbooster." to player
                play sound "minecraft:entity.villager.no" to player
    else if name of event-item is "&e&lSuper Exp Multi-Up":
        if {expmultibooster-active::%player's uuid%} and {superexpmultibooster-active::%player's uuid%} are false:
            set {superexpmultibooster-active::%player's uuid%} to true
            set player's held item to air
            set {superexpmultibooster-addition::%player's uuid%} to {expmulti::%player's uuid%}
            add {superexpmultibooster-addition::%player's uuid%} to {expmulti::%player's uuid%}
            send "&d&lCROPLITE &aYou have activated a &e&lSuper Exp Multi-Up &abooster." to player
            play sound "minecraft:block.brewing_stand.brew" with pitch 1.3 to player
            play sound "entity.experience_orb.pickup" to player
            
            wait 4 minutes
            set {superexpmultibooster-active::%player's uuid%} to false
            subtract {superexpmultibooster-addition::%player's uuid%} from {expmulti::%player's uuid%}
            send "&d&lCROPLITE &cYour &e&lSuper Exp Multi-Up &cbooster has ran out." to player
