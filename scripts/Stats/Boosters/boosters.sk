local function loadBoosterGui(p: player):
    set {_gui} to chest inventory with 3 rows named "&d&lBoosters"

    loop 27 times:
        set {_item} to purple stained glass pane
        hide {_item}'s tooltip
        set slot (loop-number - 1) of {_gui} to {_item}

    set slot 11 of {_gui} to air
    set slot 12 of {_gui} to air
    set slot 13 of {_gui} to air
    set slot 14 of {_gui} to air
    set slot 15 of {_gui} to air

    set slot 22 of {_gui} to barrier named "&8[&4&lClose&8]"
        
    open {_gui} to {_p}

function resetBoosters(p: player):
    set {expmultibooster-active::%uuid of {_p}%} to false
    set {superexpmultibooster-active::%uuid of {_p}%} to false

    set {cropmultibooster-active::%uuid of {_p}%} to false
    set {supercropmultibooster-active::%uuid of {_p}%} to false

    set {damagebooster-active::%uuid of {_p}%} to false
    set {superdamagebooster-active::%uuid of {_p}%} to false

    set {speedbooster-active::%uuid of {_p}%} to false
    set {superspeedbooster-active::%uuid of {_p}%} to false

    set {cashbooster-active::%uuid of {_p}%} to 0
    set {supercashbooster-active::%uuid of {_p}%} to 0

    if {expmultibooster-active::%uuid of {_p}%} is true:
        set {expmultibooster-active::%uuid of {_p}%} to false
        subtract 1 from {expmulti::%uuid of {_p}%}
    else if {superexpmultibooster-active::%uuid of {_p}%} is true:
        set {superexpmultibooster-active::%uuid of {_p}%} to false
        subtract {superexpmultibooster-addition::%uuid of {_p}%} from {expmulti::%uuid of {_p}%}
    else if {cropmultibooster-active::%uuid of {_p}%} is true:
        set {cropmultibooster-active::%uuid of {_p}%} to false
        subtract 1 from {cropmulti::%uuid of {_p}%}
    else if {supercropmultibooster-active::%uuid of {_p}%} is true:
        set {supercropmultibooster-active::%uuid of {_p}%} to false
        subtract {supercropmultibooster-addition::%uuid of {_p}%} from {cropmulti::%uuid of {_p}%}
    else if {damagebooster-active::%uuid of {_p}%} is true:
        set {damagebooster-active::%uuid of {_p}%} to false
        subtract 1 from {damageboost::%uuid of {_p}%}
    else if {superdamagebooster-active::%uuid of {_p}%} is true:
        set {superdamagebooster-active::%uuid of {_p}%} to false
        subtract 2 from {damageboost::%uuid of {_p}%}
    else if {speedbooster-active::%uuid of {_p}%} is true:
        set {speedbooster-active::%uuid of {_p}%} to false
        subtract {speedbooster-addition::%uuid of {_p}%} from {_p}'s walk speed
    else if {superspeedbooster-active::%uuid of {_p}%} is true:
        set {superspeedbooster-active::%uuid of {_p}%} to false
        subtract {superspeedbooster-addition::%uuid of {_p}%} from {_p}'s walk speed
    
    set {superexpmultibooster-addition::%uuid of {_p}%} to 0
    set {supercropmultibooster-addition::%uuid of {_p}%} to 0
    set {speedbooster-addition::%uuid of {_p}%} to 0
    set {superspeedbooster-addition::%uuid of {_p}%} to 0
    set {cashbooster-multiplier::%uuid of {_p}%} to 1
    set {damageboost::%uuid of {_p}%} to 0

command /boosters:
    trigger:
        send "&5&lCroplite &cComing Soon..." to player

command /getboosters [<player>]:
    permission: operator
    trigger:
        if arg-1 is set:
            give arg-1 {boosters::*}
        else:
            give player {boosters::*}